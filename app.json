{
  "expo": {
    "name": "Corona Tracker",
    "slug": "Corona-tracker",
    "version": "1.0.2", 
    "orientation": "portrait",
    "privacy":"public",
    "description":"An application to track the number of people affected in a given country",
    "icon": "./assets/Corona-img.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
